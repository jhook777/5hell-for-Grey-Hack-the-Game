        list_files(folder, grep, quiet, waitFor,depth+1)
      end for
      for file in files
        list_files(file, grep, quiet, waitFor,depth+1)
      end for
    else
      if result.name == grep then
        globals.grepped_file = result
      end if
    end if
  end if
  //return grepped_file
