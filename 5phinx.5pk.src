    for tab in g
      if typeof(tab) == "file" and tab.parent.name != ".Trash" then 
        t = tab  
      end if
    end for
    if DEBUG then 
      print "-- t: "+typeof(t)+": "+t;
      if typeof(t) == "file" then print "-- "+t.path+char(10)+"-- "+t.get_folders
    end if
    if not t or typeof(t) != "file" or not t.is_folder then return "grep: tables not found"
