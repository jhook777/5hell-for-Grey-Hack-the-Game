				end if
				file_id_info.push("lines: "+str(lines))
				file_id_info.push("chars: "+str(chars))
			end if
		end if
		file_id_info.push("symlnk: "+sym_stat)
		crypt_stat = "-crypto.so not loaded-"
		if globals.crypto then 
			if crypto.is_encrypted(arg1.path) then crypt_stat = "yes" else crypt_stat = "no"
			if arg1.is_folder then crypt_stat = "no"
		end if
		file_id_info.push("encrypted: "+crypt_stat)
		if return_enc_status then 
			print "file: encryption status for: "+char(10)+colorWhite+"</b><u>"+arg1.path+"</u>:"
