			if dec_file then act = colorViolet+"</b>decrypt"
			print colorGreen+"</b>file: "+act+"ing..." 
			//ec_files = [arg1]
			pw = arg2
			if not pw then pw = user_input(colorGold+"</b>-- encryption key:> ",1)
			if arg1.is_folder then arg1 = command.tree("-r",arg1.path)
			if DEBUG then print arg1
			ts = time
			if not arg1 or arg1.len < 1 then return "file: nothing to encrypt"
			indnt = "<size=75%>-- "
			// re-cursed code follows
			encrypt_path = function(ec_files,indnt,act,pw)
				if globals.DEBUG then print "debug: ec_files: "+ec_files
				for fl in ec_files
					if not fl.is_folder then 
						c = false
						pth = fl.path
						print indnt+act+"ing </color>"+pth
						if act == colorMagenta+"</b>encrypt" then // can't @crypto[act] -- Sadge :[
							c = globals.crypto.encrypt(pth,pw)
						else
							c = globals.crypto.decrypt(pth,pw)
						end if
						if c == true then print indnt+"</b>"+act+"ion<b> successful" else print indnt+"</b>"+act+"ion"+colorWarning+" failed!"+char(10)+indnt+colorWarning+"</b>"+c 
					else 
						encrypt_path(fl.get_folders,indnt,act,pw)
					end if
				end for
			end function
			if typeof(arg1) != "list" then arg1 = [arg1]
			encrypt_path(arg1,indnt,act,pw)
			et = str(time - ts)
			return "<size=75%>file: "+act+"ion complete in [<b>"+et+"</b>] seconds"+char(10)
		end if

    	imp_stat = null
		if DEBUG then print "checking for symlink"
