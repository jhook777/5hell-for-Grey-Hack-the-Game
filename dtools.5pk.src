	return inf
  end function
  action.t = function(inf)
	print globals.tagged_for_scp(inf.path)
	return inf 
  end function
  action.b = function(inf) // global buffer
    selected = action.get_selected(inf)
    if not selected then return inf
    globals.BUFFER.push(selected)
    conf = user_input("felix:<b> sent "+selected.name+" to BUFFER. Acces via memory_alpha.</b>"+char(10)+"press <<b>anyKey</b>> to continue",0,1)
    return inf
  end function
  action.p = function(inf) // perms
	if not action.memory then return inf
	chf = action.memory
	recurse = 0 
	if chf.is_folder and user_input("-- recursive? [<b>Y</b>/n]||: ")  != "n" then recurse = 1
	print "-- enter user{+|-}permission strings delimited by spaces:"+char(10)+
	"--eg: o-rwx g+x u+rwx"
	u_string = user_input(":> ").trim
	if u_string == "" then return inf
	u_list = u_string.split(" ")
