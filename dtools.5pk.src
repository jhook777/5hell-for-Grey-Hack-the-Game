	if DEBUG then print("cerebrum calling: pwgen false "+arg1)
	command.pwgen("false",arg1)
	time_e = (time - time_s)
	print( globals.dict_a.len+" passwords readied in "+time_e+" seconds" )
	if arg1 == "-d" or arg2 == "-d" or arg3 == "-d" then return dict_a else return 0
end function
