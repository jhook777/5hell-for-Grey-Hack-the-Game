  	to_buffer = false
	return_name = false
	return_path = false
	return_file = false
	return_size = false 
	return_enc_status = false 
	enc_file = false 
	dec_file = false
	return_parent = false
	print
	if arg1.indexOf("-") >= 0 then //== "-l" or arg1 == "-c" or arg1 == "-b" then
		if arg1 == "-e" then enc_file = true 
		if arg1 == "-d" then dec_file = true 
