	rem_usr = ""
	rem_pass = ""
	if arg1.indexOf("@") >= 0 then
		split = arg1.split("@")
		rem_usr = split[0].trim
		rem_pass = split[1].trim
	else
		return "ssh: expects user@pass [ip]"
	end if
