			end if
		end if
	
		if arg1 == "load" or arg1 == "-l" then 
			if arg2 and typeof(arg2) == "MetaLib" then 
				globals.metaLib = arg2
				print colorLightBlue+"</b><size=75%>meta: loaded new <b>MetaLib</b> object:"
