			else if typeof(confirm) == "string" and confirm.lower == "n" then
				return "aborting..."
			else
				return "invalid entry: " + @confirm
			end if
		else
			return "Error starting daemon manager"
		end if
	end if
	return "valid options: -t OR -c OR -b OR -d OR -e OR -s OR -r OR -m OR -x OR -o OR -p"
end function
