	if arg1 == "-d" then
		manager = new DaemonManager
		manager.Init
		if manager.__initialized and manager.__file then
			if not confirm then confirm = user_input("Stop daemons: are you sure? [y/N]||: ",0,1)
			if typeof(confirm) == "string" and confirm.lower == "y" then
