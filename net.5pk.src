			end if
			return 0
		end if
	
	end if
	
	if not globals.metaxploit then print "meta: metaxploit.so not found" 
	if typeof(globals.metaLib) == "MetaLib" then return globals.metaLib.lib_name + " v " +globals.metaLib.version
	return "meta: no MetaLib linked"
	
